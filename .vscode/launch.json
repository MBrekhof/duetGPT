{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch and Debug",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/duetGPT/bin/Debug/net8.0/duetGPT.dll",
            "args": [],
            "cwd": "${workspaceFolder}/duetGPT",
            "stopAtEntry": false,
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "\\bNow listening on:\\s+(https?://\\S+)",
                "uriFormat": "%s"
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        }
    ]
}
